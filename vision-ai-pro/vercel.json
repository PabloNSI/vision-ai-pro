{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/logs.html",
      "dest": "/public/logs.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/public/index.html"
    }
  ]
}